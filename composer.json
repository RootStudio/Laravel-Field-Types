{  
   "name":"rootstudio/laravel-field-types",
   "description":"Simple form builder package for creating reusable input fields in Laravel",
   "type":"library",
   "homepage":"https://github.com/RootStudio/Laravel-Field-Types",
   "authors":[  
      {  
         "name":"James Wigger",
         "email":"james@rootstudio.co.uk"
      }
   ],
   "support":{  
      "email":"james@rootstudio.co.uk",
      "issues":"https://github.com/RootStudio/Laravel-Field-Types/issues"
   },
   "require":{
      "illuminate/view": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
      "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0"
   },
   "require-dev": {
      "phpunit/phpunit": "~5.0",
      "orchestra/testbench": "~3.3"
   },
   "autoload":{  
      "psr-4":{  
         "RootStudio\\RootForms\\":"src"
      },
      "files": [
         "src/Support/helpers.php"
      ]
   },
   "autoload-dev": {
      "psr-4": {
         "RootStudio\\RootForms\\Tests\\": "tests"
      }
   }
}
